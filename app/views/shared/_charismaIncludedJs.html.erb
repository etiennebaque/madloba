<%= javascript_include_tag 'admin/bootstrap.min' %>

<!-- library for cookie management -->
<%= javascript_include_tag 'admin/jquery.cookie' %>
<!-- data table plugin -->
<%= javascript_include_tag 'admin/jquery.dataTables.min' %>
<!-- select or dropdown enhancer -->
<%= javascript_include_tag 'admin/chosen.jquery.min' %>
<!-- notification plugin -->
<%= javascript_include_tag 'admin/jquery.noty' %>
<!-- library for making tables responsive -->
<%= javascript_include_tag 'admin/responsive-tables' %>
<!-- autogrowing textarea plugin -->
<%= javascript_include_tag 'admin/jquery.autogrow-textarea' %>
<!-- history.js for cross-browser state change on ajax -->
<%= javascript_include_tag 'admin/jquery.history' %>
<!-- application script for Charisma demo -->
<%= javascript_include_tag 'admin/charisma' %>

<%= javascript_include_tag 'admin/admin' %>


<!-- jquery_ujs is already included in application.js. However, some Bootstrap themes might override it, causing
 functionalities like Devise sign out no to work. Therefore, we include jquery_ujs after loading all scripts -->
<%= javascript_include_tag 'jquery_ujs' %>